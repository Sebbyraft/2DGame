package Timer;

public class Clock {
	
	private int minutes;
	private int hours;
	
	public Clock(int hours, int minutes) {
		this.hours = hours;
		this.minutes = minutes;
	}
	
	public void update() {
		
	}

	public int getMinutes() {
		return minutes;
	}

	public void setMinutes(int minutes) {
		this.minutes = minutes;
	}

	public int getHours() {
		return hours;
	}

	public void setHours(int hours) {
		this.hours = hours;
	}
	
	

}
